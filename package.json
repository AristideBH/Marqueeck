{
  "name": "@arisbh/marqueeck",
  "version": "0.1.12",
  "description": "Marqueeck is a custom and customizable marquee component for Sveltekit",
  "author": {
    "name": "AristideBH",
    "email": "aristide.bruneau@gmail.com",
    "url": "https://github.com/AristideBH"
  },
  "homepage": "https://marqueeck.vercel.app",
  "keywords": [
    "sveltekit",
    "marquee",
    "typescript",
    "svelte3",
    "sveltejs",
    "svelte-component"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "pubup": "pnpm publish && pnpm update && git commit -am 'Package.json update after npm publish'",
    "v:patch": "git commit -am 'patch:version update' && pnpm version patch ",
    "v:minor": "git commit -am 'minor:version update' && pnpm version minor",
    "v:major": "git commit -am 'major:version update' && pnpm version major"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@arisbh/marqueeck": "0.1.12",
    "@skeletonlabs/skeleton": "^1.5.1",
    "@sveltejs/adapter-auto": "^2.0.1",
    "@sveltejs/kit": "^1.16.3",
    "@sveltejs/package": "^2.0.2",
    "@types/node": "^20.1.2",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.28.0",
    "highlight.js": "^11.8.0",
    "postcss": "^8.4.23",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "publint": "^0.1.11",
    "svelte": "^3.59.1",
    "svelte-check": "^3.3.2",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}